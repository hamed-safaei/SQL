SELECT t1.list_price , t2.order_date , t3.first_name , t3.last_name ,t3.email , t4.product_name , t5.category_name FROM sales.order_items as t1
INNER JOIN sales.orders as t2 ON t1.order_id = t2.order_id
INNER JOIN sales.customers as t3 on t2.customer_id = t3.customer_id
INNER JOIN production.products as t4 ON t1.product_id = t4.product_id
INNER JOIN production.categories as t5 ON t4.category_id = t5.category_id
